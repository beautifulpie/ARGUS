{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ARGUS Eye Track Frame",
  "type": "object",
  "required": ["trackId", "timestampMs", "samples", "sampleRateHz"],
  "properties": {
    "trackId": {
      "type": "string",
      "minLength": 1
    },
    "timestampMs": {
      "type": "integer",
      "minimum": 0
    },
    "sampleRateHz": {
      "type": "number",
      "exclusiveMinimum": 0
    },
    "samples": {
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "meta": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
